{
  "name": "fastify-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.9.1",
  "dependencies": {
    "fastify": "^5.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.14.1",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "yarn lint && tsc",
    "ts-check": "tsc --noEmit",
    "lint": "eslint .",
    "validate": "yarn lint && yarn ts-check",
    "format": "prettier --write .",
    "postinstall": "husky"
  }
}
