{
  "name": "fastify-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "fastify": "^5.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.18",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "yarn lint && tsc",
    "ts-check": "tsc --noEmit",
    "lint": "eslint .",
    "validate": "yarn lint && yarn ts-check",
    "format": "prettier --write .",
    "postinstall": "husky"
  }
}
